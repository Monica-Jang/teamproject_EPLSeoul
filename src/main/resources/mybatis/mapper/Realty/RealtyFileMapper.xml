			(
			`RCPT_YR`,
			`CGG_CD`,
			`CGG_NM`,
			`STDG_CD`,
			`STDG_NM`,
			`LOTNO_SE`,
			`LOTNO_SE_NM`,
			`MNO`,
			`SNO`,
			`BLDG_NM`,
			`CTRT_DAY`,
			`THING_AMT`,
			`ARCH_AREA`,
			`LAND_AREA`,
			`FLR`,
			`RGHT_SE`,
			`RTRCN_DAY`,
			`ARCH_YR`,
			`BLDG_USG`,
			`DCLR_SE`,
			`OPBIZ_RESTAGNT_SGG_NM`
			)
			VALUES
			(
			#{rcpt_yr},
			#{cgg_cd},
			#{cgg_nm},
			#{stdg_cd},
			#{stdg_nm},
			#{lotno_se},
			#{lotno_se_nm},
			#{mno},
			#{sno},
			#{bldg_nm},
			#{ctrt_day},
			#{thing_amt},
			#{arch_area},
			#{land_area},
			#{flr},
			#{rght_se},
			#{rtrcn_day},
			#{arch_yr},
			#{bldg_usg},
			#{dclr_se},
			#{opbiz_restagnt_sgg_nm}
			)
	</insert>

	<delete id="buyingDeleteAll">
		DELETE FROM `EPL_SEOUL`.`REALTY_BUYING`
	</delete>
	
	<select id="getBuyingCount" resultType="_int">
		SELECT COUNT(*) FROM `EPL_SEOUL`.`REALTY_BUYING`
	</select>
	
</mapper>